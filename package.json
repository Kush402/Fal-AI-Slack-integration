{
  "name": "slack-ai-asset-generator",
  "version": "1.0.0",
  "description": "A Slack-integrated AI dashboard bot for teams at Paradowski Creative to generate marketing and creative assets using fal.ai's multimodal models",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:backend": "node src/server.js",
    "start:slack": "node src/slack-service.js",
    "dev": "nodemon src/server.js",
    "dev:backend": "nodemon src/server.js",
    "dev:slack": "nodemon src/slack-service.js",
    "pm2:start": "./scripts/pm2-manager.sh start",
    "pm2:stop": "./scripts/pm2-manager.sh stop",
    "pm2:restart": "./scripts/pm2-manager.sh restart",
    "pm2:delete": "./scripts/pm2-manager.sh delete",
    "pm2:logs": "./scripts/pm2-manager.sh logs",
    "pm2:monit": "./scripts/pm2-manager.sh monitor",
    "pm2:status": "./scripts/pm2-manager.sh status",
    "pm2:health": "./scripts/pm2-manager.sh health",
    "pm2:scale": "./scripts/pm2-manager.sh scale",
    "pm2:backend": "pm2 start ecosystem.config.js --only dashboard-backend",
    "pm2:slack": "pm2 start ecosystem.config.js --only dashboard-slack",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "setup:logs": "mkdir -p logs",
    "log:cleanup": "node scripts/log-cleanup.js",
    "log:cleanup:dry": "node scripts/log-cleanup.js --dry-run",
    "log:stats": "node -e \"require('./src/utils/logManager').getLogStats().then(console.log)\"",
    "port:check": "node -e \"require('./src/utils/portManager').getPortStatus().then(console.log)\""
  },
  "keywords": [
    "slack",
    "ai",
    "fal.ai",
    "gemini",
    "asset-generation",
    "creative",
    "mcp"
  ],
  "author": "kush.sharma",
  "license": "MIT",
  "dependencies": {
    "@fal-ai/client": "^1.5.0",
    "@google-cloud/aiplatform": "^4.3.0",
    "@google-cloud/storage": "^7.7.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/genai": "^1.9.0",
    "@google/generative-ai": "^0.24.1",
    "@slack/bolt": "^4.4.0",
    "@slack/socket-mode": "^2.0.4",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bull": "^4.12.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-list-endpoints": "^7.1.1",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.0",
    "googleapis": "^133.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.1",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "chai": "^5.2.1",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "mocha": "^11.7.1",
    "nodemon": "^3.0.2",
    "pm2": "^6.0.8",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paradowski-creative/slack-ai-asset-generator.git"
  },
  "bugs": {
    "url": "https://github.com/paradowski-creative/slack-ai-asset-generator/issues"
  },
  "homepage": "https://github.com/paradowski-creative/slack-ai-asset-generator#readme"
}
